{
  "name": "aditya-car-zone",
  "version": "1.0.0",
  "description": "A modern React TypeScript car marketplace application",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "start": "webpack serve --mode development",
    "start:alt": "webpack serve --config webpack.dev.config.js",
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,css,json}\"",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified\" && exit 0",
    "prepare": "husky install"
  },
  "keywords": [
    "react",
    "typescript",
    "webpack",
    "car-marketplace",
    "modern-ui"
  ],
  "author": "Aditya",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/aditya-car-zone.git"
  },
  "dependencies": {
    "@types/styled-components": "^5.1.34",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "html-webpack-plugin": "^5.6.4",
    "husky": "^9.1.7",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,json}": [
      "prettier --write"
    ]
  }
}
